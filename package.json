{
	"name": "sv-router",
	"version": "0.0.2",
	"description": "Modern Svelte routing",
	"keywords": [
		"svelte",
		"router",
		"spa"
	],
	"homepage": "https://github.com/colinlienard/sv-router",
	"bugs": {
		"url": "https://github.com/colinlienard/sv-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/colinlienard/sv-router.git"
	},
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"sv-router/source": "./src/index.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./vite-plugin": {
			"import": "./dist/vite-plugin/index.js",
			"types": "./dist/vite-plugin/index.d.ts"
		}
	},
	"bin": "./dist/cli/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"ex:basic": "pnpm --filter basic-example",
		"ex:file-based": "pnpm --filter file-based-example",
		"build": "svelte-package -i src && rm -rf .svelte-kit",
		"watch": "svelte-package -i src --watch",
		"test": "vitest",
		"check": "tsc --noEmit && pnpm -r check",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write"
	},
	"dependencies": {
		"esm-env": "^1.1.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@sveltejs/package": "^2.3.7",
		"@types/node": "^22.10.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^2.46.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"globals": "^15.12.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.2",
		"type-testing": "^0.2.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.14.0",
		"vite": "^5.4.10",
		"vitest": "^2.1.5"
	},
	"peerDependencies": {
		"svelte": "^5"
	},
	"packageManager": "pnpm@9.15.0"
}
