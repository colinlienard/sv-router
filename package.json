{
	"name": "sv-router",
	"version": "0.12.0",
	"description": "Modern Svelte Routing",
	"keywords": [
		"svelte",
		"router",
		"spa"
	],
	"homepage": "https://github.com/colinlienard/sv-router",
	"bugs": {
		"url": "https://github.com/colinlienard/sv-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/colinlienard/sv-router.git"
	},
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./src/index.js",
			"svelte": "./src/index.js",
			"types": "./src/index.d.ts"
		},
		"./vite-plugin": {
			"import": "./src/vite-plugin/index.js",
			"types": "./src/vite-plugin/index.d.ts"
		}
	},
	"bin": "./src/cli/index.js",
	"files": [
		"src"
	],
	"scripts": {
		"create:dev": "pnpm --filter create-sv-router dev",
		"create:build": "pnpm --filter create-sv-router build",
		"docs:dev": "pnpm --filter docs dev",
		"docs:build": "pnpm --filter docs build",
		"docs:preview": "pnpm --filter docs preview",
		"test": "vitest",
		"check": "svelte-check && pnpm -r check",
		"lint": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"changeset": "changeset"
	},
	"dependencies": {
		"esm-env": "^1.2.2"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@eslint/js": "^9.39.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.13.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"globals": "^16.5.0",
		"happy-dom": "^20.0.11",
		"prettier": "^3.7.4",
		"prettier-plugin-jsdoc": "^1.8.0",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte-check": "^4.3.4",
		"type-testing": "^0.2.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vitest": "^4.0.15"
	},
	"peerDependencies": {
		"svelte": "^5"
	},
	"packageManager": "pnpm@10.24.0"
}
